+++ b/target/linux/mediatek/dts/mt7986a-netcore-n60-pro.dts
@@ -7,7 +7,7 @@
 #include "mt7986a.dtsi"
 
 / {
-	model = "Netcore N60 PRO";
+	model = "Netcore N60 PRO 512ROM";
 	compatible = "netcore,n60-pro", "mediatek,mt7986a";
 
 	aliases {
@@ -241,7 +241,7 @@
 
 			partition@580000 {
 				label = "ubi";
-				reg = <0x0580000 0x7280000>;
+				reg = <0x0580000 0x1ea00000>;
 			};
 		};
 	};
